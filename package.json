{
  "name": "react-boilerplate",
  "version": "0.0.0",
  "description": "A boilerplate for a React app with TypeScript, ESLint, and Jest, bundled by Webpack with swc.",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prebuild": "rm -rf build",
    "build": "cross-env APP_ENV=production pnpm prebuild && webpack --config ./webpack.config/index.ts",
    "develop": "cross-env APP_ENV=development webpack serve --config ./webpack.config/index.ts",
    "lint": "eslint src",
    "test": "jest",
    "typecheck": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drumnickydrum/react-boilerplate.git"
  },
  "keywords": [
    "react",
    "boilerplate"
  ],
  "author": "Nick Carbone",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/drumnickydrum/react-boilerplate/issues"
  },
  "homepage": "https://github.com/drumnickydrum/react-boilerplate#readme",
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.194",
    "@swc/jest": "^0.2.21",
    "@swc/register": "^0.1.10",
    "@types/convict": "^6.1.1",
    "@types/convict-format-with-validator": "^6.0.2",
    "@types/jest": "^28.1.0",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.36",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "autoprefixer": "^10.4.7",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.17.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^28.1.0",
    "jest-mock": "^28.1.0",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss": "^8.4.14",
    "postcss-loader": "^7.0.0",
    "react-refresh": "^0.14.0",
    "regenerator-runtime": "^0.13.9",
    "style-loader": "^3.3.1",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.8.0",
    "typescript": "~4.6.4",
    "val-loader": "^5.0.0",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.2",
    "webpack-merge": "^5.8.0",
    "webpackbar": "^5.0.2"
  },
  "dependencies": {
    "convict": "^6.2.3",
    "convict-format-with-validator": "^6.2.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
